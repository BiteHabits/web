table password {
  id text [pk, not null]
  userId text [not null]
  hash text [not null]
}

table session {
  id text [pk, not null]
  userId text [not null]
  expiresAt integer [not null]
  createdAt integer [not null]
}

table user {
  id text [pk, not null]
  name text [not null]
  email text [not null, unique]

  indexes {
    email [name: 'email_idx']
  }
}

ref: user.id - password.userId

ref: session.userId > user.id